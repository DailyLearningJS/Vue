{
  "name": "vue-demo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "mocha",
    "deeptest": "mocha --recursive",
    "btest": "mocha --recursive --reporter mochawesome"
  },
  "dependencies": {
    "vue": "^2.6.8"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "element-ui": "^2.5.4",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.5.0",
    "mocha": "^6.0.2",
    "mochawesome": "^3.1.1",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "superagent": "^4.1.0",
    "vue-template-compiler": "^2.6.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
